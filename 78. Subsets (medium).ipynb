   "source": [
    "class Solution:\n",
    "    def subsets(self, nums):\n",
    "        if not nums:\n",
    "            return []\n",
    "        if len(nums) == 1:\n",
    "            return [[nums[0]],[]]\n",
    "\n",
    "        subres = self.subsets(nums[1:])\n",
    "        to_add = [x+[nums[0]] for x in subres]\n",
    "        res = subres + to_add\n",
    "        return res"
   ]
